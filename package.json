{
  "name": "@deep-foundation/capacitor-device",
  "keywords": [
    "deep-package"
  ],
  "version": "1.0.3",
  "description": "A deep package that is based on @capacitor/device package",
  "repository": {
    "type": "git",
    "url": "https://github.com/deep-foundation/capacitor-device"
  },
  "scripts": {
    "npm-pull": "(git diff --quiet || (echo 'You have unstaged changes. Stash them (git stash) or commit them (git commit)'; exit 1)) && npm install ${npm_package_name}@latest --no-save && mv --force node_modules/${npm_package_name}/* ./",
    "sync-versions": "npx ts-node ./automation/sync-versions.ts",
    "build": "tsc"
  },
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "/dist",
    "./deep.json"
  ],
  "license": "Unlicense",
  "dependencies": {
    "@deep-foundation/core": "^0.0.2",
    "@freephoenix888/boolean": "^0.0.1",
    "@freephoenix888/object-to-links-async-converter": "^0.0.10",
    "delay": "^5.0.0",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "@capacitor/device": "^4.1.0",
    "@chakra-ui/react": "^2.5.5",
    "@deep-foundation/deeplinks": "~0.0.164",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "inquirer": "^9.2.6",
    "ts-node": "^10.9.1"
  }
}
